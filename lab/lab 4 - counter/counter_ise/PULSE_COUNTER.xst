set -tmpdir "./xst/projnav.tmp"
set -xsthdpdir "./xst"
run
-ifn PULSE_COUNTER.prj
-ifmt mixed
-ofn PULSE_COUNTER
-ofmt NGC
-p xa9500xl
-top PULSE_COUNTER
-opt_mode Speed
-opt_level 1
-iuc NO
-lso PULSE_COUNTER.lso
-keep_hierarchy YES
-rtlview Yes
-hierarchy_separator /
-bus_delimiter <>
-case maintain
-verilog2001 YES
-fsm_extract YES -fsm_encoding Auto
-safe_implementation No
-mux_extract YES
-resource_sharing YES
-iobuf YES
-pld_mp YES
-pld_xp YES
-pld_ce YES
-wysiwyg NO
-equivalent_register_removal YES
